{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualMachines_VM_group_93ae4b66_name": {
            "defaultValue": "VM-group_93ae4b66",
            "type": "String"
        },
        "virtualMachineScaleSets_VM_group_externalid": {
            "defaultValue": "/subscriptions/188533e8-ed54-47d2-857a-0620ebdeb029/resourceGroups/Devoir1/providers/Microsoft.Compute/virtualMachineScaleSets/VM-group",
            "type": "String"
        },
        "disks_VM_group_93ae4b66_OsDisk_1_bfec9c5ce4084e0e86d54f99b782b9f3_externalid": {
            "defaultValue": "/subscriptions/188533e8-ed54-47d2-857a-0620ebdeb029/resourceGroups/Devoir1/providers/Microsoft.Compute/disks/VM-group_93ae4b66_OsDisk_1_bfec9c5ce4084e0e86d54f99b782b9f3",
            "type": "String"
        },
        "networkInterfaces_VM1_vnet_nic01_f404804e_externalid": {
            "defaultValue": "/subscriptions/188533e8-ed54-47d2-857a-0620ebdeb029/resourceGroups/Devoir1/providers/Microsoft.Network/networkInterfaces/VM1-vnet-nic01-f404804e",
            "type": "String"
        },
        "adminPassSecret": {
            "type": "string"
        },
        "secret1Secret": {
            "type": "string"
        },
        "keyVaultResourceId": {
            "type": "string"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Compute/virtualMachines",
            "apiVersion": "2024-07-01",
            "name": "[parameters('virtualMachines_VM_group_93ae4b66_name')]",
            "location": "eastus",
            "tags": {
                "VirtualMachineProfileTimeCreated": "11/5/2024 12:51:55 AM +00:00"
            },
            "zones": [
                "1"
            ],
            "properties": {
                "hardwareProfile": {
                    "vmSize": "Standard_DC1s_v2"
                },
                "virtualMachineScaleSet": {
                    "id": "[parameters('virtualMachineScaleSets_VM_group_externalid')]"
                },
                "additionalCapabilities": {
                    "hibernationEnabled": false
                },
                "storageProfile": {
                    "imageReference": {
                        "publisher": "canonical",
                        "offer": "ubuntu-24_04-lts",
                        "sku": "server",
                        "version": "latest"
                    },
                    "osDisk": {
                        "osType": "Linux",
                        "name": "[concat(parameters('virtualMachines_VM_group_93ae4b66_name'), '_OsDisk_1_bfec9c5ce4084e0e86d54f99b782b9f3')]",
                        "createOption": "FromImage",
                        "caching": "ReadWrite",
                        "managedDisk": {
                            "id": "[parameters('disks_VM_group_93ae4b66_OsDisk_1_bfec9c5ce4084e0e86d54f99b782b9f3_externalid')]"
                        },
                        "deleteOption": "Delete"
                    },
                    "dataDisks": [],
                    "diskControllerType": "SCSI"
                },
                "osProfile": {
                    "computerName": "vm-groupsU1YX0A",
                    "adminUsername": "coursCloud",
                    "linuxConfiguration": {
                        "disablePasswordAuthentication": false,
                        "provisionVMAgent": true,
                        "patchSettings": {
                            "patchMode": "ImageDefault",
                            "assessmentMode": "ImageDefault"
                        }
                    },
                    "secrets": [
                        {
                            "sourceVault": {
                                "id": "[parameters('keyVaultResourceId')]"
                            },
                            "vaultSecrets": [
                                {
                                    "secretUrl": "[parameters('adminPassSecret')]"
                                },
                                {
                                    "secretUrl": "[parameters('secret1Secret')]"
                                }
                            ]
                        }
                    ],
                    "allowExtensionOperations": true,
                    "requireGuestProvisionSignal": true
                },
                "securityProfile": {
                    "uefiSettings": {
                        "secureBootEnabled": true,
                        "vTpmEnabled": true
                    },
                    "securityType": "TrustedLaunch"
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[parameters('networkInterfaces_VM1_vnet_nic01_f404804e_externalid')]",
                            "properties": {
                                "primary": true,
                                "deleteOption": "Delete"
                            }
                        }
                    ]
                },
                "diagnosticsProfile": {
                    "bootDiagnostics": {
                        "enabled": true
                    }
                }
            }
        }
    ]
}
